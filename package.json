{
  "name": "mshop-holoo-relay",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./build/index.js",
    "compile": "tsc && node ./build/src/index.js",
    "dev": "nodemon -e ts --watch src --exec \"npm run compile\"",
    "build": "tsc -w",
    "dev2": "nodemon ./build/index.js -e ts,js",
    "dev3": "tsc && nodemon ./src/index.ts -e js ",
    "dev4": "node --watch ./build/index.js"
  },
  "type": "module",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "msexcel-builder": "^0.0.2",
    "mssql": "^10.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/mssql": "^8.1.2",
    "@types/node": "17.0.42",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "4.7.3"
  }
}
